   "source": [
    "using Primes\n",
    "\n",
    "function twinprimes(m)\n",
    "  results = []\n",
    "  n = 4\n",
    "  while length(results) < m\n",
    "    isprime(n-1) && isprime(n+1) && push!(results, (n-1, n+1))\n",
    "    n += 2\n",
    "  end\n",
    "  results\n",
    "end"
   ]
